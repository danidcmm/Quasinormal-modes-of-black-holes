Cell[TextData[{
 "The solutions, without considering the terms in \[Epsilon], are cylindrical \
functions ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["D", "\[Nu]"], "(", "t", ")"}], TraditionalForm]],
  ExpressionUUID->"f77655ea-187f-444b-91d5-ee8207112dcc"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["D", 
     RowBox[{
      RowBox[{"-", "\[Nu]"}], "-", "1"}]], "(", "it", ")"}], 
   TraditionalForm]],ExpressionUUID->"d58dab61-b401-4faa-a5ad-8109c89ca713"],
 ". We will therefore look for solutions of the type ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"f", "(", "t", ")"}], 
    RowBox[{
     SubscriptBox["D", "\[Nu]"], "[", " ", 
     RowBox[{"g", "(", "t", ")"}], " ", "]"}]}], TraditionalForm]],
  ExpressionUUID->"db651d67-7de1-4f38-a773-76e0f1298a99"],
 " when considering the terms in \[Epsilon]. Choosing ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "=", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"dg", "/", "dt"}], ")"}], 
     RowBox[{"1", "/", "2"}]]}], TraditionalForm]],ExpressionUUID->
  "54b5041e-7324-4912-b6f3-ebf08e2385f7"],
 " and substituing in the differential equation (see [2] for details) we \
finally obtain this expression:"
}], "Text",
 CellGroupingRules->{"GroupTogetherGrouping", 10005.},
 CellChangeTimes->{
  3.833521677796751*^9, {3.8335217884969254`*^9, 3.8335219352468863`*^9}, {
   3.833522115674635*^9, 3.833522117336796*^9}, {3.8335995009488153`*^9, 
   3.8335995271319113`*^9}},ExpressionUUID->"00426230-6455-4424-b655-\
2c380ec46402"],

Cell[BoxData[
 RowBox[{
  RowBox[{"diffeq", "=", 
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{
       RowBox[{"g", "'"}], "[", "t", "]"}], "4"], 
     RowBox[{"(", 
      RowBox[{"\[Nu]", "+", 
       FractionBox["1", "2"], "-", 
       RowBox[{
        FractionBox["1", "4"], 
        SuperscriptBox[
         RowBox[{"g", "[", "t", "]"}], "2"]}]}], ")"}]}], "+", 
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{
      RowBox[{"g", "'''"}], "[", "t", "]"}], 
     RowBox[{
      RowBox[{"g", "'"}], "[", "t", "]"}]}], "-", 
    RowBox[{
     FractionBox["3", "4"], 
     SuperscriptBox[
      RowBox[{
       RowBox[{"g", "''"}], "[", "t", "]"}], "2"]}], "-", 
    RowBox[{
     RowBox[{"q", "[", "t", "]"}], 
     SuperscriptBox[
      RowBox[{
       RowBox[{"g", "'"}], "[", "t", "]"}], "2"]}]}]}], ";"}]], "Input",
