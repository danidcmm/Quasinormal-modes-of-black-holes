     SuperscriptBox[
      RowBox[{"(", 
       FractionBox["1", "r"], ")"}], 
      RowBox[{"1", "+", "\[Delta]"}]]}], ")"}], 
   FractionBox[
    RowBox[{"(", 
     RowBox[{"1.", "+", "\[Delta]"}], ")"}], 
    RowBox[{"1.", "+", 
     RowBox[{"\[Delta]", 
      RowBox[{"(", 
       RowBox[{"1.", "+", 
        FractionBox["1", "r"]}], ")"}]}]}]]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Agop", "[", "y_", "]"}], ":=", 
  RowBox[{"-", 
   RowBox[{"(", 
    RowBox[{"1", "-", 
     FractionBox["1", 
      RowBox[{"y", "+", "x0"}]], "+", 
     RowBox[{
      FractionBox["\[CapitalDelta]", 
       RowBox[{"4", "\[Pi]"}]], 
      FractionBox[
       RowBox[{"1", " "}], 
       RowBox[{" ", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"y", "+", "x0"}], ")"}], "6"], 
         SuperscriptBox[
