Cell[BoxData[
 RowBox[{
  StyleBox[
   RowBox[{"q", "[", "t_", "]"}], "Code"], 
  StyleBox[":=", "Code"], 
  RowBox[{
   RowBox[{"\[Nu]", "+", 
    FractionBox["1", "2"], "-", 
    RowBox[{
     FractionBox["1", "4"], 
     SuperscriptBox["t", "2"]}], "-", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", 
      RowBox[{"1", "/", "2"}]], "\[Zeta]3", " ", 
     SuperscriptBox["t", "3"]}], "+", 
    RowBox[{"\[Epsilon]", " ", 
     RowBox[{"(", 
      RowBox[{"\[CapitalLambda]", "-", 
       RowBox[{"\[Zeta]4", " ", 
        SuperscriptBox["t", "4"]}]}], ")"}]}], "-", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", 
      RowBox[{"3", "/", "2"}]], "\[Zeta]5", " ", 
     SuperscriptBox["t", "5"]}], "+", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "2"], 
     RowBox[{"(", 
      RowBox[{"\[CapitalOmega]", "-", 
       RowBox[{"\[Zeta]6", " ", 
        SuperscriptBox["t", "6"]}]}], ")"}]}], "-", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", 
      RowBox[{"5", "/", "2"}]], "\[Zeta]7", " ", 
     SuperscriptBox["t", "7"]}], "+", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "3"], " ", 
     RowBox[{"(", 
      RowBox[{"\[CapitalPhi]", "-", 
       RowBox[{"\[Zeta]8", " ", 
        SuperscriptBox["t", "8"]}]}], ")"}]}], "-", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", 
      RowBox[{"7", "/", "2"}]], "\[Zeta]9", " ", 
     SuperscriptBox["t", "9"]}], "+", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "4"], 
     RowBox[{"(", 
      RowBox[{"\[CapitalTheta]", "-", 
       RowBox[{"\[Zeta]10", " ", 
        SuperscriptBox["t", "10"]}]}], ")"}]}], "-", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", 
      RowBox[{"9", "/", "2"}]], "\[Zeta]11", " ", 
     SuperscriptBox["t", "11"]}], "+", 
    RowBox[{
     SuperscriptBox["\[Epsilon]", "5"], 
     RowBox[{"(", 
      RowBox[{"\[CapitalXi]", "-", 
       RowBox[{"\[Zeta]12", " ", 
        SuperscriptBox["t", "12"]}]}], ")"}]}]}], "//", 
   "Evaluate"}]}]], "Input",
